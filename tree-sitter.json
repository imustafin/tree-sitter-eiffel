{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "eiffel",
      "camelcase": "Eiffel",
      "title": "Eiffel",
      "scope": "source.eiffel",
      "file-types": [
        "e"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^eiffel$",
      "class-name": "TreeSitterEiffel"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Eiffel grammar for tree-sitter",
    "authors": [
      {
        "name": "Ilgiz Mustafin",
        "email": "ilgimustafin@gmail.com",
        "url": "https://imustafin.tatar/"
      },
      {
        "name": "Alessandro Schena"
      }
    ],
    "links": {
      "repository": "https://github.com/imustafin/tree-sitter-eiffel"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
